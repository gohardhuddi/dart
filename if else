void main() {
  //if else leader
 var marks=-5;
  if(marks>=90 && marks<100)
  {
    print("A+");
  }
  else if(marks>=80 && marks<90)
  {
    print("A");
  }
  else if(marks>=70 && marks<80)
  {
    print("B+");
  }
  else if(marks>=60 && marks<70)
  {
    print("B");
  }
  else if(marks>=50 && marks<60)
  {
    print("C+");
  }
  else if(marks<50 && marks>0)
  {
    print("f");
  }
  else
  {
    print("invaled statement");
  }
}
